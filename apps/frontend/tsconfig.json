{
  "$schema": "https://www.schemastore.org/jsconfig.json",
  "extends": [
    "../../jsconfig.json"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "jsx": "react-jsx",
    "jsxImportSource": "@jsxrx/core",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/*.ts",
    "./src/*.tsx"
  ]
}
